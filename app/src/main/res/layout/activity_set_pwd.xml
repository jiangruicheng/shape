<?xml version="1.0" encoding="utf-8"?>
<layout>
<com.zhy.android.percent.support.PercentLinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/bg"
    android:orientation="vertical"
    tools:context="com.cndll.shapetest.activity.SetPwdActivity">

    <include
        android:id="@+id/titlebar"
        layout="@layout/titlebar"
        android:layout_width="0dp"
        android:layout_height="0dp"
        app:layout_heightPercent="8%h"
        app:layout_widthPercent="100%w" />


    <TextView
        android:id="@+id/auth_type"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:text="请选择认证身份"
        android:textColor="@color/text"
        app:layout_heightPercent="6%h"
        android:layout_gravity="center"
        android:gravity="center_vertical"
        app:layout_textSizePercent="@string/text_size_mid"
        app:layout_widthPercent="90%w"
        />

    <com.zhy.android.percent.support.PercentLinearLayout
        android:id="@+id/pwd_sig"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="1dp"
        app:layout_widthPercent="100%w"
        app:layout_heightPercent="8%h"
        android:background="@color/white"
        android:orientation="horizontal"
        app:layout_paddingPercent="1%">

        <TextView
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            app:layout_marginTopPercent="2%h"
            app:layout_marginBottomPercent="2%h"
            android:gravity="center"
            android:text="修改登录密码"
            android:textColor="@color/contents_text"
            app:layout_textSizePercent="@string/text_size_big"
            app:layout_widthPercent="30%w"
            />

        <ImageView
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_gravity="center"
            android:src="@mipmap/right"
            app:layout_paddingPercent="3%"
            app:layout_marginLeftPercent="60%"
            app:layout_widthPercent="10%w" />
    </com.zhy.android.percent.support.PercentLinearLayout>

    <com.zhy.android.percent.support.PercentLinearLayout
        android:id="@+id/pwd_pay"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="1dp"
        app:layout_widthPercent="100%w"
        app:layout_heightPercent="8%h"
        android:background="@color/white"
        android:orientation="horizontal"
        app:layout_paddingPercent="1%">

        <TextView
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            app:layout_marginTopPercent="2%h"
            app:layout_marginBottomPercent="2%h"
            android:gravity="center"
            android:text="修改支付密码"
            android:textColor="@color/contents_text"
            app:layout_textSizePercent="@string/text_size_big"
            app:layout_widthPercent="30%w"
            />

        <ImageView
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_gravity="center"
            android:src="@mipmap/right"
            app:layout_paddingPercent="3%"
            app:layout_marginLeftPercent="60%"
            app:layout_widthPercent="10%w" />
    </com.zhy.android.percent.support.PercentLinearLayout>

    <com.zhy.android.percent.support.PercentLinearLayout
        android:id="@+id/pwd_copy"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="1dp"
        app:layout_widthPercent="100%w"
        app:layout_heightPercent="8%h"
        android:background="@color/white"
        android:orientation="horizontal"
        app:layout_paddingPercent="1%">

        <TextView
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            app:layout_marginTopPercent="2%h"
            app:layout_marginBottomPercent="2%h"
            android:gravity="center"
            android:text="版权信息"
            android:textColor="@color/contents_text"
            app:layout_textSizePercent="@string/text_size_big"
            app:layout_widthPercent="25%w"
            />

        <ImageView
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_gravity="center"
            android:src="@mipmap/right"
            app:layout_paddingPercent="3%"
            app:layout_marginLeftPercent="65%"
            app:layout_widthPercent="10%w" />
    </com.zhy.android.percent.support.PercentLinearLayout>

    <com.zhy.android.percent.support.PercentLinearLayout
        android:id="@+id/pwd_deal"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="1dp"
        app:layout_widthPercent="100%w"
        app:layout_heightPercent="8%h"
        android:background="@color/white"
        android:orientation="horizontal"
        app:layout_paddingPercent="1%">

        <TextView
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            app:layout_marginTopPercent="2%h"
            app:layout_marginBottomPercent="2%h"
            android:gravity="center"
            android:text="软件许可使用协议"
            android:textColor="@color/contents_text"
            app:layout_textSizePercent="@string/text_size_big"
            app:layout_widthPercent="40%w"
            />

        <ImageView
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_gravity="center"
            android:src="@mipmap/right"
            app:layout_paddingPercent="3%"
            app:layout_marginLeftPercent="50%"
            app:layout_widthPercent="10%w" />
    </com.zhy.android.percent.support.PercentLinearLayout>

    <com.zhy.android.percent.support.PercentLinearLayout
        android:id="@+id/pwd_help"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="1dp"
        app:layout_widthPercent="100%w"
        app:layout_heightPercent="8%h"
        android:background="@color/white"
        android:orientation="horizontal"
        app:layout_paddingPercent="1%">

        <TextView
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            app:layout_marginTopPercent="2%h"
            app:layout_marginBottomPercent="2%h"
            android:gravity="center"
            android:text="帮助中心"
            android:textColor="@color/contents_text"
            app:layout_textSizePercent="@string/text_size_big"
            app:layout_widthPercent="25%w"
            />

        <ImageView
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_gravity="center"
            android:src="@mipmap/right"
            app:layout_paddingPercent="3%"
            app:layout_marginLeftPercent="65%"
            app:layout_widthPercent="10%w" />
    </com.zhy.android.percent.support.PercentLinearLayout>

    <com.zhy.android.percent.support.PercentLinearLayout
        android:id="@+id/lin_user"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="1dp"
        app:layout_widthPercent="100%w"
        app:layout_heightPercent="8%h"
        android:background="@color/white"
        android:orientation="horizontal"
        app:layout_paddingPercent="1%">

        <TextView
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            app:layout_marginTopPercent="2%h"
            app:layout_marginBottomPercent="2%h"
            android:gravity="center"
            android:text="个人"
            android:textColor="@color/contents_text"
            app:layout_textSizePercent="@string/text_size_big"
            app:layout_widthPercent="25%w"
            />

        <ImageView
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_gravity="center"
            android:src="@mipmap/right"
            app:layout_paddingPercent="3%"
            app:layout_marginLeftPercent="65%"
            app:layout_widthPercent="10%w" />
    </com.zhy.android.percent.support.PercentLinearLayout>
    <com.zhy.android.percent.support.PercentLinearLayout
        android:id="@+id/lin_enterprise"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="1dp"
        app:layout_widthPercent="100%w"
        app:layout_heightPercent="8%h"
        android:background="@color/white"
        android:orientation="horizontal"
        app:layout_paddingPercent="1%">

        <TextView
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            app:layout_marginTopPercent="2%h"
            app:layout_marginBottomPercent="2%h"
            android:gravity="center"
            android:text="企业"
            android:textColor="@color/contents_text"
            app:layout_textSizePercent="@string/text_size_big"
            app:layout_widthPercent="25%w"
            />

        <ImageView
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_gravity="center"
            android:src="@mipmap/right"
            app:layout_paddingPercent="3%"
            app:layout_marginLeftPercent="65%"
            app:layout_widthPercent="10%w" />
    </com.zhy.android.percent.support.PercentLinearLayout>




</com.zhy.android.percent.support.PercentLinearLayout>
</layout>